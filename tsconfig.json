{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true
  },
  "ts-node": {
    "swc": true
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
